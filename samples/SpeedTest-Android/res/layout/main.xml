    <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="horizontal">
        <TextView android:id="@+id/gson_full_min_label" android:text="Min:" android:layout_width="match_parent" android:gravity="right" android:layout_weight="33" android:layout_height="wrap_content"/>
        <TextView android:id="@+id/gson_full_max_label" android:text="Max:" android:layout_width="match_parent" android:gravity="right" android:layout_weight="33" android:layout_height="wrap_content"/>
        <TextView android:id="@+id/gson_full_avg_label" android:text="Avg:" android:layout_width="match_parent" android:gravity="right" android:layout_weight="33" android:layout_height="wrap_content"/>
