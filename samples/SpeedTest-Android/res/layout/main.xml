        <TextView android:id="@+id/gson_full_max_value" android:text="--" android:layout_width="match_parent" android:gravity="left" android:layout_weight="33" android:layout_height="wrap_content"/>
        <TextView android:id="@+id/gson_full_avg_value" android:text="--" android:layout_width="match_parent" android:gravity="left" android:layout_weight="33" android:layout_height="wrap_content"/>
    </LinearLayout>
