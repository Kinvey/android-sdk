<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent"
        >

    <ScrollView android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">

    <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="horizontal">

        <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Request Count"
                android:textStyle="bold"
                android:layout_weight="50"
                android:gravity="center"
                android:id="@+id/title_requestcount"

                />

        <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Entity Count"
                android:layout_weight="50"
                android:gravity="center"
                android:textStyle="bold"
                android:id="@+id/title_entitycount"

                />


    </LinearLayout>

    <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="horizontal">

        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="12"
                android:gravity="center"
                android:layout_weight="50"
                android:id="@+id/value_requestcount"

                />

        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="12"
                android:gravity="center"
                android:layout_weight="50"
                android:id="@+id/value_entitycount"

                />


    </LinearLayout>






    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="View inflation Time"
            android:textStyle="bold"
            android:id="@+id/title_views"

            />

    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="--"
            android:gravity="right"
            android:id="@+id/value_views"

            />


    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="GSON Client Build Time"
            android:textStyle="bold"
            android:id="@+id/gson_title_build"

            />

    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="--"
            android:gravity="right"
            android:id="@+id/gson_value_build"
            />



    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="GSON Login Time"
            android:textStyle="bold"
            android:id="@+id/gson_title_login"

            />

    <TextView
            android:layout_width="match_parent"
