            />

    <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="horizontal">
        <TextView android:id="@+id/gson_empty_min_label" android:text="Min:" android:layout_width="match_parent" android:layout_weight="33" android:layout_height="wrap_content"/>
        <TextView android:id="@+id/gson_empty_max_label" android:text="Max:" android:layout_width="match_parent" android:layout_weight="33" android:layout_height="wrap_content"/>
        <TextView android:id="@+id/gson_empty_avg_label" android:text="Avg:" android:layout_width="match_parent" android:layout_weight="33" android:layout_height="wrap_content"/>
    </LinearLayout>

    <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:orientation="horizontal">
        <TextView android:id="@+id/gson_empty_min_value" android:text="--" android:layout_width="match_parent" android:layout_weight="33" android:layout_height="wrap_content"/>
        <TextView android:id="@+id/gson_empty_max_value" android:text="--" android:layout_width="match_parent" android:layout_weight="33" android:layout_height="wrap_content"/>
        <TextView android:id="@+id/gson_empty_avg_value" android:text="--" android:layout_width="match_parent" android:layout_weight="33" android:layout_height="wrap_content"/>
    </LinearLayout>


    <TextView
            android:layout_width="match_parent"
